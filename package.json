{
  "name": "ts-console-boilerplate",
  "version": "1.0.0",
  "description": "Boilerplate for console with TypeScript, eslint, prettier, jest, vscode",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "devDependencies": {
    "@types/jest": "^27.0.3",
    "@types/node": "^16.11.12",
    "@typescript-eslint/eslint-plugin": "^5.6.0",
    "@typescript-eslint/parser": "^5.6.0",
    "eslint": "^8.4.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.4.4",
    "npm-check-updates": "^12.0.3",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.1.1",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.3"
  },
  "scripts": {
    "test": "jest --config jestconfig.json",
    "clean": "rimraf dist/",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "eslint . --ext .ts",
    "start": "ts-node ./src/index.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pablocar80/ts-console-boilerplate.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/pablocar80/ts-console-boilerplate/issues"
  },
  "homepage": "https://github.com/pablocar80/ts-console-boilerplate#readme",
  "dependencies": {
    "standard-collections": "^1.4.4"
  }
}
